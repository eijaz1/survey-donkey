register.erb

<form action='/register' method='POST'>
  <div>
    <input type="text" name="user[email]" placeholder="Email Address">
  </div>
  <div>
    <select name="user[gender]">
      <option selected disabled>Gender</option>
      <option value="male">Male</option>
      <option value="female">Female</option>
      <!-- <option value="other">Other</option> -->
    </select>
  </div>
  <div>
    <select name="user[age]">
      <option selected disabled>Age</option>
      <option value="young">18-35</option>
      <option value="middle">36-54</option>
      <option value="old">55+</option>
    </select>
    </div>
    <div>
      <select name="user[location]">
        <option selected disabled>Location</option>
        <option value="northeast">Northeast</option>
        <option value="south">South</option>
        <option value="midwest">Midwest</option>
        <option value="west">West</option>
      </select>
    </div>
    <div>
      <select name="user[race_ethnicity]">
        <option selected disabled>Race/Ethnicity</option>
        <option value="white">White</option>
        <option value="black">Black/African American</option>
        <option value="asian">Asian</option>
        <option value="hispanic">Hispanic</option>
        <option value="other">Other</option>
      </select>
    </div>
      <div>
        <select name="user[religion]">
          <option selected disabled>Religion</option>
          <option value="christian">Christian</option>
          <option value="jewish">Jewish</option>
          <option value="muslim">Muslim</option>
          <option value="hindu">Hindu</option>
          <option value="agnostic">Agnostic</option>
          <option value="atheist">Atheist</option>
          <option value="other">Other</option>
        </select>
    </div>
      <div>
        <select name="user[party_affiliation]">
          <option selected disabled>Party Affiliation</option>
          <option value="democrat">Democrat</option>
          <option value="republican">Republican</option>
          <option value="independent">Independent</option>
        </select>
    </div>
    <div>
        <input type="password" name="user[password]" placeholder="Password">
    </div>
    
    <div>
        <input type="submit" value="Register!">
    </div>
  </form>



